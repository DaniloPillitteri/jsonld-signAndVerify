{
    "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",
        
        "Car":{
            "@id": "https://schema.org/Car",
            "@context": {
                "@version": 1.1,
                "@protected": true,

                "id": "@id",
                "type": "@type",
                "color": {
                    "@id": "https://schema.org/color",
                    "@type": "@id"
                },
                "speed": {
                    "@id": "https://schema.org/speed",
                    "@type": "@id"
                }
            }
        },

        "proof": {
            "@id": "https://w3id.org/security#proof",
            "@type": "@id",
            "@container": "@graph"
        },
        "Property": "https://uri.etsi.org/ngsi-ld/Property",
        "value": "https://uri.etsi.org/ngsi-ld/hasValue",
        "values": {
            "@id": "ngsi-ld:hasValues",
            "@container": "@list"
          }  
    }
